application/msword; antiword '%s'; copiousoutput; description=Word Document; nametemplate=%s.doc

application/pdf; pdftotext '%s' -; copiousoutput; description=PDF Document; nametemplate=%s.pdf

application/pgp-keys; pgp -f < %s; copiousoutput; description=PGP Key

text/html; w3m -o auto_image=TRUE -I %{charset} -T text/html; copiousoutput; description=HTML
text/html; firefox %s; nametemplate=%s.html; description=HTML

image/*; feh --scale-down %s; description=Image
image/*; convert %s jpg:- | jp2a --width=76 -; copiousoutput; description=Image

video/*; mpv %s > /dev/null; description=Video
video/*; ffmpegthumbnailer -i %s -c jpeg -o - | jp2a --width=76 -; copiousoutput; description=Video
